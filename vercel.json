{
  "devCommand": "vite --port $PORT",
  "buildCommand": "vite build",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    {
      "source": "/(@vite|@react-refresh|node_modules/.vite|src/.*|assets/.*)",
      "destination": "/$1"
    },
    {
      "source": "/((?!api|@vite|@react-refresh|node_modules/.vite|assets|.*\\..*).*)",
      "destination": "/"
    }
  ]
}
